{
  "expo": {
    "name": "prepeer",
    "slug": "prepeer",
    "version": "1.0.0",
    "scheme": "prepeer",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/prepeer.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/prepeer.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/prepeer.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff",
      "width": 50,
      "height": 50,
      "padding": 50
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logoWithWriteup.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "androidStatusBar": "transparent"
      }
    }
  }
}
